        <div class="tab-content">
            <!--Setup-->
            <div class="tab-pane active" id="setup">
                <form class="form-horizontal">
                    <div class="control-group">
                        <label class="control-label" for="printerName">Printer Name</label>
                        <div class="controls">
                            <input type="text" class="midsizeTextbox" id="printerName" data-bind="value: printer_name">
                        </div>
                    </div>
                    <div class="control-group">
                        <label class="control-label" for="filamentUsed">Filament Used</label>
                        <div class="controls">
                            <select class="midsizeTextbox" id="filamentUsed" data-bind="value: filament_used">
                                <option value="PLA">PLA</option>
                                <option value="ABS">ABS</option>
                                <option value="PETG">PETG</option>
                                <option value="TPU">TPU</option>
                            </select>
                        </div>
                    </div>
                    <div class="control-group">
                        <label class="control-label" for="filamentDiameter">Filament Diameter</label>
                        <div class="controls">
                            <div class="input-append">
                                <input type="text" class="midsizeTextbox" id="filamentDiameter" data-bind="value: filament_diameter" placeholder="(mm)">
                                <span class="add-on">mm</span>
                            </div>
                        </div>
                    </div>
                </form>

